{
  "Config: Text/Email/Number": {
    "prefix": ["conf:text", "conf:email", "conf:mail", "conf:number"],
    "body": [
      "<div class=\"form-group\">",
      "\t<label class=\"form-label\" for=\"${2}\">${3:Label}</label>",
      "\t<input type=\"${5:text,email,number}\" id=\"${2:text}\" aria-describedby=\"${2}-help\" class=\"form-control\" name=\"${1:text}\">",
      "\t<p id=\"${2}-help\" class=\"help-block\">${4:Help text}</p>",
      "</div>"
    ],
    "description": "Standard text/email/number field"
  },
